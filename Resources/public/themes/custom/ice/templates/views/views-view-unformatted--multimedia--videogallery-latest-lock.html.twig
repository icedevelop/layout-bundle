{% extends "views-view-unformatted--multimedia.html.twig" %}

{% block content %}
  {% for row in rows %}
    {% set row_classes = [
    default_row_class ? 'views-row',
    'Grid-cell',
    'u-text-r-m',
    'u-margin-r-bottom',
    'u-layout-matchHeight'
    ] %}
    <div{{ row.attributes.addClass(row_classes) }}>
      {{ row.content }}
    </div>
  {% endfor %}
{% endblock %}
